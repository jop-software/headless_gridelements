{
  "name": "itplusx/headless-gridelements",
  "type": "typo3-cms-extension",
  "description": "Grid Elements json output for EXT:headless",
  "keywords": [
    "TYPO3",
    "TYPO3 CMS",
    "extension",
    "gridelements",
    "headless"
  ],
  "homepage": "https://itplusx.de",
  "support": {
    "issues": "https://github.com/itplusx/headless_gridelements/issues"
  },
  "authors": [
    {
      "name": "RamÃ³n Schlosser",
      "email": "schlosser@itplusx.de",
      "role": "Developer",
      "homepage": "https://itplusx.de"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "typo3/cms-core": "^11.0",
    "friendsoftypo3/headless": "dev-master",
    "gridelementsteam/gridelements": "dev-bugfix/wizard-193"
  },
  "autoload": {
    "psr-4": {
      "ITplusX\\HeadlessGridelements\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ITplusX\\HeadlessGridelements\\Tests\\": "Tests"
    }
  },
  "replace": {
    "typo3-ter/headless_gridelements": "self.version",
    "itplusx/headless-gridelements": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    },
    "typo3/cms": {
      "extension-key": "headless_gridelements"
    }
  },
  "repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/jop-software/headless_gridelements"
		}
	]
}
